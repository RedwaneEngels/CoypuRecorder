Class {
	#name : #DoubleRythmAnnouncement,
	#superclass : #Announcement,
	#instVars : [
		'symbol'
	],
	#category : #'CoypuRecorder-Announcements'
}

{ #category : #accessing }
DoubleRythmAnnouncement >> symbol [

	^symbol
]

{ #category : #accessing }
DoubleRythmAnnouncement >> symbol: aSymbol [

symbol := aSymbol
]
